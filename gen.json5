{
    "envfile": {
        "env": ".env",
        "sample": ".env-sample",
        "enabled": true
    },
    "database": {
        "init": "docker\/POSTGRES\/init.sql",
        "type": "postgres",
        "enabled": true
    },
    "code": {
        "package": {
            "base": "com.jantoni1.recruitmentapp",
            "namingStrategy": "lowercase"
        },
        "queues": {
            "type": "rabbitmq",
            "enabled": true
        },
        "domain": {
            "baseException": {
                "type": "java.lang.RuntimeException"
            }
        },
        "language": "java",
        "jpa": {
            "baseSpecification": {
                "type": "org.springframework.data.jpa.domain.Specification"
            },
            "baseRepository": {
                "type": "org.springframework.data.jpa.repository.JpaRepository"
            },
            "baseEntity": {
                "type": "com.jantoni1.recruitmentapp.jpa.BaseEntity"
            },
            "enabled": false
        },
        "permission": {
            "defaultExpression": "Permission.disallow()",
            "spelValidator": "@permission.validate(?)",
            "type": "pl.valueadd.permission.Permission",
            "enabled": true
        },
        "id": {
            "defaultValue": "new UUID(0,0)",
            "type": "java.util.UUID",
            "nullValue": "UUID.randomUUID()"
        },
        "event": {
            "listener": {
                "method": "handle",
                "type": "com.jantoni1.recruitmentapp.eventdispatcher.EventListener",
                "enabled": true
            },
            "event": {},
            "dispatcher": {
                "method": "dispatch",
                "type": "com.jantoni1.recruitmentapp.eventdispatcher.EventDispatcher",
                "enabled": true
            },
            "enabled": true
        },
        "user": {
            "provider": {
                "type": "com.jantoni1.recruitmentapp.userprovider.CurrentUserProvider"
            },
            "displayNameMethod": "getDisplayName",
            "type": "com.jantoni1.recruitmentapp.userprovider.User"
        },
        "caching": {
            "type": "redis",
            "ttl": 10,
            "enabled": true
        },
        "dto": {
            "baseRequest": {
                "type": "com.jantoni1.recruitmentapp.request.dto.BaseRequest"
            }
        }
    },
    "buildTool": {
        "type": "maven"
    },
    "project": {
        "groupName": "com.jantoni1",
        "name": "recruitment_app"
    },
    "containers": {
        "type": "docker",
        "enabled": true,
        "docker": {
            "deps": "docker-compose.deps.yml",
            "apps": "docker-compose.apps.yml"
        }
    },
    "version": 2
}